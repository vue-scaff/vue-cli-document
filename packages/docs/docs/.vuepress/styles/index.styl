.friendship
	text-align: center;
	margin-top: 128px;
	display: flex;
	flex-direction: row;
	justify-content: center;

	& > span, & > a
		display: flex;
		flex-direction: column;
		cursor: pointer;
		padding: 0 60px;
		transition: color 0.36s;

		&:hover
			color: #3eaf7c;

		& > img
			border-radius: 6px;
			box-shadow: 0 0 6px rgba(0, 0, 0, .25);
